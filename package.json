{
  "name": "spotscan",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:client",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:server": "cd src/server && tsnd --respawn --watch *.ts --clear src/app.ts",
    "start:client": "ng serve --proxy-config proxy.conf.json --open",
    "build:server": "cd src/server && tsc",
    "build:client": "ng build",
    "build:all": "npm run build:server && npm run build:client",
    "start:built": "node dist/app.js",
    "build:all:start": "npm run build:all && npm run start:built"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.4",
    "@angular/common": "~10.2.4",
    "@angular/compiler": "~10.2.4",
    "@angular/core": "~10.2.4",
    "@angular/forms": "~10.2.4",
    "@angular/google-maps": "^12.0.1",
    "@angular/platform-browser": "~10.2.4",
    "@angular/platform-browser-dynamic": "~10.2.4",
    "@angular/router": "~10.2.4",
    "@overnightjs/core": "^1.7.4",
    "@rmp135/imgur": "^0.5.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "instagram-private-api": "^1.43.3",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.10.18",
    "node-fetch": "^2.6.0",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "tsyringe": "^4.3.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular/cli": "~10.2.3",
