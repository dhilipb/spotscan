{
  "name": "spotscan",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:client",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:server": "cd src/server && tsnd --respawn --rs --watch *.ts --clear src/app.ts",
    "start:client": "ng serve --proxy-config proxy.conf.json",
    "build:server": "cd src/server && tsc",
    "build:client": "ng build",
    "build:all": "npm run build:client && npm run build:server",
    "start:built": "node dist/server/src/app.js",
    "build:all:start": "npm run build:all && npm run start:built"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.9",
