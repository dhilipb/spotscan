    "@types/express": "^4.17.6",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/mongodb": "^3.5.2",
    "@types/mongoose": "^5.7.29",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
