    "@typegoose/typegoose": "^7.2.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/mongodb": "^3.5.34",
    "@types/mongoose": "^5.7.29",
    "@types/node": "^12.19.7",
    "codelyzer": "^5.1.2",
