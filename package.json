    "@angular/forms": "~9.1.9",
    "@angular/google-maps": "^9.2.4",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@overnightjs/core": "^1.7.4",
    "@typegoose/typegoose": "^7.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase-admin": "^8.12.1",
    "instagram-private-api": "^1.40.1",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
